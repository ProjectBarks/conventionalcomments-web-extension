{
  "name": "conventionalcomments-web-ext",
  "license": "GPL-3.0-only",
  "version": "1.0.3",
  "scripts": {
    "postinstall": "get-google-fonts -o ./public/fonts -i 'https://fonts.googleapis.com/css?family=Roboto:300,400,500&display=swap'",
    "rollup:popup": "rollup -c rollup/popup.config.js",
    "rollup:contentScript": "rollup -c rollup/contentScript.config.js",
    "web-ext:build": "web-ext build --overwrite-dest",
    "build": "npm-run-all -p rollup:** -s web-ext:build",
    "dev": "run-p 'rollup:** -- -w'",
    "start": "sirv public --dev",
    "lint": "npm-run-all lint:**",
    "lint:eslint": "eslint --max-warnings 0 --ignore-path .gitignore --ignore-pattern *.d.ts --ext .ts,.js,.svelte .",
    "lint:prettier": "prettier -c --ignore-path .gitignore **/*.svelte **/*.css",
    "lint:tsc": "tsc --noEmit",
    "lint:svelte": "svelte-check --fail-on-warnings --fail-on-hints",
    "test": "jest",
    "codeceptjs": "codeceptjs run --steps",
    "version": "node scripts/newVersion.js && git add public/manifest.json",
    "postversion": "git push origin master && git push origin refs/tags/v$npm_package_version"
  },
  "devDependencies": {
    "@babel/core": "7.14.0",
    "@babel/preset-env": "7.14.0",
    "@codeceptjs/configure": "0.6.2",
    "@codeceptjs/helper": "1.0.2",
    "@gitbeaker/node": "28.3.0",
    "@rollup/plugin-commonjs": "18.0.0",
    "@rollup/plugin-node-resolve": "11.2.1",
    "@rollup/plugin-typescript": "8.2.1",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/svelte": "3.0.3",
    "@tsconfig/svelte": "1.0.10",
    "@types/jest": "26.0.23",
    "@types/marked": "2.0.2",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "babel-jest": "26.6.3",
    "chrome-webstore-upload-cli": "1.2.1",
    "codeceptjs": "3.0.5",
    "config": "3.3.6",
    "eslint": "7.16.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-codeceptjs": "1.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-svelte3": "3.2.0",
    "get-google-fonts": "1.2.2",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "playwright": "1.3.0",
    "prettier": "2.2.1",
    "prettier-plugin-svelte": "1.4.2",
    "rollup": "2.35.1",
    "rollup-plugin-css-only": "3.1.0",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-svelte": "7.0.0",
    "rollup-plugin-terser": "7.0.2",
    "sirv-cli": "1.0.10",
    "svelte": "3.31.0",
    "svelte-check": "1.1.23",
    "svelte-jester": "1.3.0",
    "svelte-preprocess": "4.7.3",
    "svelte-select": "3.15.0",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "tslib": "2.0.3",
    "typescript": "4.1.3",
    "web-ext": "6.1.0",
    "web-ext-submit": "6.1.0"
  },
  "dependencies": {
    "@mdi/js": "5.9.55",
    "chota": "0.8.0",
    "escape-string-regexp": "4.0.0",
    "marked": "2.0.3",
    "svelte-chota": "1.8.4",
    "uuid": "8.3.2",
    "webextension-polyfill-ts": "0.25.0"
  },
  "webExt": {
    "sourceDir": "public"
  },
  "private": true
}

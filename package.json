{
  "name": "conventionalcomments-web-ext",
  "license": "GPL-3.0-only",
  "version": "1.1.0",
  "scripts": {
    "postinstall": "get-google-fonts -o ./public/fonts -i 'https://fonts.googleapis.com/css?family=Roboto:300,400,500&display=swap' && node scripts/postInstall.js",
    "rollup:popup": "rollup -c rollup/popup.config.js",
    "rollup:contentScript": "rollup -c rollup/contentScript.config.js",
    "web-ext:build": "web-ext build --overwrite-dest",
    "build": "npm-run-all -p rollup:** -s web-ext:build",
    "dev": "run-p 'rollup:** -- -w'",
    "start": "sirv public --dev",
    "lint": "npm-run-all lint:**",
    "lint:eslint": "eslint --max-warnings 0 --ignore-path .gitignore --ignore-pattern *.d.ts --ext .ts,.js,.svelte .",
    "lint:prettier": "prettier -c --ignore-path .gitignore **/*.svelte **/*.css",
    "lint:tsc": "tsc --noEmit",
    "lint:svelte": "svelte-check --fail-on-warnings --fail-on-hints",
    "test": "jest",
    "codeceptjs": "codeceptjs run --steps",
    "codeceptjs:updateScreenshots": "run-s codeceptjs:updateScreenshots:**",
    "codeceptjs:updateScreenshots:github": "E2E_PRODUCT=github E2E_UPDATE_SCREENSHOTS=true codeceptjs run --steps -f 'Visual'",
    "codeceptjs:updateScreenshots:gitlab": "E2E_PRODUCT=gitlab E2E_UPDATE_SCREENSHOTS=true codeceptjs run --steps -f 'Visual'",
    "codeceptjs:ci": "codeceptjs run --steps --reporter mochawesome",
    "version": "node scripts/newVersion.js && git add public/manifest.json",
    "postversion": "git push origin master && git push origin refs/tags/v$npm_package_version",
    "github-2fa-code": "node scripts/getGithubTwoFactorAuthenticationCode.js"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/preset-env": "7.15.4",
    "@codeceptjs/helper": "1.0.2",
    "@gitbeaker/node": "28.3.0",
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-node-resolve": "13.0.4",
    "@rollup/plugin-typescript": "8.2.5",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/svelte": "3.0.3",
    "@tsconfig/svelte": "2.0.1",
    "@types/jest": "27.0.1",
    "@types/marked": "2.0.2",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "babel-jest": "27.1.0",
    "chrome-webstore-upload-cli": "1.2.1",
    "codeceptjs": "3.2.2",
    "codeceptjs-resemblehelper": "1.9.4",
    "config": "3.3.6",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-codeceptjs": "1.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-svelte3": "3.2.0",
    "get-google-fonts": "1.2.2",
    "jest": "27.1.0",
    "lodash.get": "4.4.2",
    "lodash.merge": "4.6.2",
    "mocha": "9.1.1",
    "mochawesome": "6.2.2",
    "npm-run-all": "4.1.5",
    "otplib": "12.0.1",
    "playwright": "1.17.1",
    "prettier": "2.3.2",
    "prettier-plugin-svelte": "2.4.0",
    "rollup": "2.56.3",
    "rollup-plugin-css-only": "3.1.0",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-svelte": "7.1.0",
    "rollup-plugin-terser": "7.0.2",
    "sirv-cli": "1.0.14",
    "svelte": "3.42.4",
    "svelte-check": "1.1.23",
    "svelte-jester": "2.1.1",
    "svelte-preprocess": "4.9.1",
    "svelte-select": "4.4.1",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "tslib": "2.3.1",
    "typescript": "4.3.5",
    "web-ext": "6.3.0",
    "web-ext-submit": "6.3.0"
  },
  "dependencies": {
    "@mdi/js": "5.9.55",
    "@types/node": "16.7.10",
    "chota": "0.8.0",
    "escape-string-regexp": "4.0.0",
    "marked": "2.0.3",
    "octokit": "1.3.0",
    "svelte-chota": "1.8.4",
    "uuid": "8.3.2",
    "webextension-polyfill-ts": "0.25.0"
  },
  "webExt": {
    "sourceDir": "public"
  },
  "private": true
}
